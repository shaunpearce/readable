{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.22.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build": "npm-run-all -p build-css build-js",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-css": "stylus -I ./node_modules -w ./src/styles/stylus/index.styl -o ./src/styles/css/index.css",
    "build-css": "stylus -I ./node_modules ./src/styles/stylus/index.styl -o ./src/styles/css/index.css"
  }
